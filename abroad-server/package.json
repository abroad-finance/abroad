{
  "name": "abroad",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate": "tsoa spec-and-routes",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate",
    "build": "npm run generate && tsc && npm run build:admin-assets",
    "build:admin-assets": "tsx scripts/build-admin-assets.ts",
    "start": "node dist/server.js",
    "start:server": "npm run start",
    "start:consumers": "node dist/consumers.js",
    "start:listeners": "node dist/listeners.js",
    "start:ws": "node dist/ws.js",
    "start:all": "concurrently -n server,consumers,listeners,ws -c auto \"npm run start\" \"npm run start:consumers\" \"npm run start:listeners\" \"npm run start:ws\"",
    "dev": "concurrently -n api,tsoa -c auto \"tsx watch src/server.ts\" \"nodemon --watch src --watch tsoa.json --ext ts,tsx,json --delay 200ms --exec \\\"tsoa spec-and-routes\\\"\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "depcruise:graph": "depcruise src --include-only '^src' --config --output-type dot | dot -T svg > dependency-graph.svg",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "hygiene": "depcheck && knip --config knip.json",
    "migrate:dev": "prisma migrate dev",
    "seed:dev": "tsx scripts/seed-dev.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
