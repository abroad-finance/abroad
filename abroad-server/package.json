{
  "name": "abroad",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app/http/server.js",
  "scripts": {
    "generate": "tsoa spec-and-routes",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate",
    "build": "npm run generate && tsc -p tsconfig.build.json",
    "start": "node dist/app/http/server.js",
    "start:server": "npm run start",
    "start:consumers": "node dist/app/workers/consumers/server.js",
    "start:outbox": "node dist/app/workers/outbox/server.js",
    "start:listeners": "node dist/app/workers/listeners/server.js",
    "start:ws": "node dist/app/workers/websocket/server.js",
    "start:all": "concurrently -n server,consumers,listeners,ws,outbox -c auto \"npm run start\" \"npm run start:consumers\" \"npm run start:listeners\" \"npm run start:ws\" \"npm run start:outbox\"",
    "dev": "concurrently -n api,tsoa -c auto \"tsx watch src/app/http/server.ts\" \"nodemon --watch src --watch tsoa.json --ext ts,tsx,json --delay 200ms --exec \\\"tsoa spec-and-routes\\\"\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:mutation": "stryker run",
    "depcruise:graph": "depcruise src --include-only '^src' --config --output-type dot | dot -T svg > dependency-graph.svg",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "maintainability:report": "tsx scripts/maintainability-report.ts --min-average 57",
    "checks:mandatory": "npm run format && npm run build && npm run test && npm run knip && npm run maintainability:report",
    "hygiene": "depcheck && knip --config knip.json",
    "knip": "knip --config knip.json",
    "migrate:dev": "prisma migrate dev",
    "seed:dev": "tsx scripts/seed-dev.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@binance/wallet": "^8.0.0",
    "@google-cloud/pubsub": "^5.2.0",
    "@google-cloud/secret-manager": "^6.1.0",
    "@prisma/client": "^6.14.0",
    "@sentry/node": "^10.38.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "@stellar/stellar-sdk": "^13.3.0",
    "@tsoa/runtime": "^6.6.0",
    "@types/express-session": "^1.18.2",
    "adminjs": "^7.8.17",
    "axios": "^1.11.0",
    "binance": "^3.0.5",
    "body-parser": "^2.2.0",
    "bs58": "^6.0.0",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ethers": "^5.7.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "firebase-admin": "^13.4.0",
    "gcp-metadata": "^7.0.1",
    "http-errors": "^2.0.0",
    "inversify": "^7.7.1",
    "ioredis": "^5.7.0",
    "js-sha512": "^0.9.0",
    "jsonwebtoken": "^9.0.2",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "tweetnacl": "^1.0.3",
    "typhonjs-escomplex": "^0.1.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@stryker-mutator/api": "^9.4.0",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@stryker-mutator/typescript-checker": "^9.4.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/commander": "^2.12.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.43.0",
    "concurrently": "^9.2.0",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^17.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "jest": "^30.0.5",
    "knip": "^5.73.4",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
