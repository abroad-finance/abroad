    "@stylistic/eslint-plugin": "^5.2.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/commander": "^2.12.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
