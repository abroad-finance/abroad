{
  "$schema": "https://unpkg.com/knip@^5/schema.json",
  "entry": [
    "src/app/http/server.ts",
    "src/app/workers/consumers/server.ts",
    "src/app/workers/listeners/server.ts",
    "src/app/workers/websocket/server.ts",
    "scripts/maintainability-report.ts",
    "scripts/seed-dev.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "scripts/**/*.ts"
  ],
  "ignore": [
    "src/scripts/**",
    "dist/**",
    "coverage/**",
    "reports/**",
    "stryker-tmp/**",
    "tmp/**"
  ],
  "ignoreDependencies": [
    "@stryker-mutator/core",
    "concurrently",
    "depcheck",
    "nodemon",
    "prisma",
    "tsx"
  ],
  "ignoreBinaries": [
    "tsoa",
    "prisma",
    "tsc",
    "concurrently",
    "tsx",
    "nodemon",
    "jest",
    "stryker",
    "depcruise",
    "dot",
    "eslint",
    "depcheck",
    "knip"
  ],
  "jest": {
    "config": [
      "jest.config.js"
    ],
    "entry": [
      "src/tests/**/*.{ts,tsx}"
    ],
    "project": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "stryker": {
    "config": [
      "stryker.config.cjs"
    ]
  }
}
